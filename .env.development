# Development Environment Configuration for Enhanced Nutrition System
# Supabase Development Database

# ==============================================
# DATABASE CONFIGURATION
# ==============================================
DATABASE_URL=postgresql://postgres.cadeererdjwemspkeriq:&vh1NzI6wwiUx@aws-0-eu-north-1.pooler.supabase.com:6543/postgres

# ==============================================
# ENHANCED NUTRITION SYSTEM SETTINGS
# ==============================================

# Master switch for all enhanced AI nutrition features
USE_ENHANCED_AI=true

# Lower threshold for development (easier testing)
DNA_CONFIDENCE_THRESHOLD=0.5

# Prediction horizon
PREDICTION_HORIZON_DAYS=7

# ==============================================
# FEATURE FLAGS - DEVELOPMENT
# ==============================================
ENABLE_BEHAVIOR_PREDICTION=true
ENABLE_CONTEXTUAL_ANALYSIS=true
ENABLE_VISUALIZATION=true
COLLECT_ENHANCED_DATA=true

# ==============================================
# DEVELOPMENT SPECIFIC SETTINGS
# ==============================================
ENVIRONMENT=development
LOG_LEVEL=DEBUG
ENHANCED_NUTRITION_DEBUG_LOGGING=true

# More aggressive caching for development performance
NUTRITION_DNA_CACHE_TTL=1800
MEAL_RECOMMENDATIONS_CACHE_TTL=900

# Allow experimental features in development
ENABLE_EXPERIMENTAL_LEARNING=false
ENABLE_AB_TESTING=false
ENHANCED_FEATURES_ROLLOUT_PERCENTAGE=100

# ==============================================
# API CONFIGURATION
# ==============================================
API_PUBLIC_URL=http://api:8020
INTERNAL_API_TOKEN=test-token-12345-for-testing-purposes-only

# ==============================================
# PRIVACY & SECURITY - DEVELOPMENT
# ==============================================
ANONYMIZE_BEHAVIORAL_DATA=false  # Keep full data for development debugging
BEHAVIORAL_DATA_RETENTION_DAYS=365  # Shorter retention for development

# ==============================================
# PERFORMANCE SETTINGS
# ==============================================
MAX_DNA_GENERATION_CONCURRENCY=3  # Lower for development resources
COLLECT_PERFORMANCE_METRICS=true